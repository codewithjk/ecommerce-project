<%- include("./layouts/header") %>
<script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js"></script>

<script src="https://unpkg.com/jspdf@latest/dist/jspdf.umd.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.debug.js"></script>
<div id="layout-wrapper">
  <%- include("./layouts/navbar") %> <%- include("./layouts/cart") %>

  <!-- ========== App Menu ========== -->
  <%-include("./layouts/removeFromCart") %>
  <!-- Left Sidebar End -->

  <div class="main-content ms-5">
    <div class="page-content">
      <div class="container-fluid">
        <!-- start page title -->
        <div class="row">
          <div class="col-12">
            <div
              class="page-title-box d-sm-flex align-items-center justify-content-between"
            >
              <h4 class="mb-sm-0">Overview</h4>

              <div class="page-title-right">
                <ol class="breadcrumb m-0">
                  <li class="breadcrumb-item">
                    <a onclick="history.back();">Orders</a>
                  </li>
                  <li class="breadcrumb-item active">Overview</li>
                </ol>
              </div>
            </div>
            <% if(order.is_cancelled){%>
            <h4 class="text-danger">Order cancelled !</h4>
            <%} %>
          </div>
        </div>
        <!-- end page title -->

        <div class="row mb-4 align-items-center">
          <div class="col"></div>
          <div class="col text-end d-flex justify-content-end gap-2">
            <% if(order.status === "Pending" && order.paymentStatus ===
            false){%>
            <button
              type="button"
              class="btn btn-secondary"
              id="btn-continue-payment"
            >
              <i class="bi bi-currency-rupee me-1 align-middle"></i> Pay now
            </button>
            <%} %> <% if(order.status === "Delivered") {%>
            <button
              type="button"
              class="btn btn-secondary"
              data-bs-toggle="modal"
              data-bs-target="#invoiceModal"
            >
              <i class="ph-download-simple me-1 align-middle"></i> Invoice
            </button>
            <%}%>
          </div>
        </div>

        <div class="row">
          <div class="col-xxl-3 col-lg-6">
            <div class="card bg-success bg-opacity-10 border-0">
              <div class="card-body">
                <div class="d-flex gap-3">
                  <div class="flex-grow-1">
                    <h6 class="fs-18 mb-3">Customer Info</h6>
                    <p class="mb-0 fw-medium"><%=order.customer %></p>
                    <p class="mb-1"><%=user.email %></p>
                    <% if(user.phoneNumber){%>
                    <p class="mb-0"><%=user.phoneNumber %></p>
                    <% } %>
                  </div>
                  <div class="avatar-sm flex-shrink-0">
                    <div
                      class="avatar-title bg-success-subtle text-success rounded fs-3"
                    >
                      <i class="ph-user-circle"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-xxl-3 col-lg-6">
            <div class="card bg-primary bg-opacity-10 border-0">
              <div class="card-body">
                <div class="d-flex gap-3">
                  <div class="flex-grow-1">
                    <h6 class="fs-18 mb-3">Shipping Address</h6>
                    <p class="mb-0"><%=order.address.addressLine1 %></p>
                    <p class="mb-1">
                      <%=order.address.city%>, <%=order.address.state %>
                    </p>
                    <p class="mb-0"><%=order.address.phoneNumber%></p>
                  </div>
                  <div class="avatar-sm flex-shrink-0">
                    <div
                      class="avatar-title bg-primary-subtle text-primary rounded fs-3"
                    >
                      <i class="ph-map-pin"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-xxl-12">
            <div class="card">
              <div class="card-body">
                <div class="table-responsive table-card">
                  <table class="table align-middle table-nowrap mb-0">
                    <thead class="text-muted table-light">
                      <tr>
                        <th scope="col">Product Name</th>
                        <th></th>
                        <th scope="col">Amount</th>
                        <th scope="col">Quantity</th>
                        <th scope="col" class="text-end">Total</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% order.products.forEach(product => {%>
                      <tr>
                        <td>
                          <div class="d-flex align-items-center">
                            <div class="flex-shrink-0 me-2">
                              <img
                                src="<%=product.image %>"
                                alt=""
                                class="avatar-xs rounded-circle"
                              />
                            </div>
                            <div class="flex-grow-1"><%=product.title %></div>
                          </div>
                        </td>
                        <td>
                          <% const returnObj =
                          JSON.stringify({orderId:order._id,product:product,})
                          %> <% if(order.status==="Delivered" &&
                          !product.is_returned){%>
                          <button
                            data-custom-data="<%=returnObj %>"
                            data-bs-toggle="modal"
                            data-bs-target="#returnProductModal"
                            type="button"
                            class="btn btn-primary"
                          >
                            <i class="bi bi-reply-fill"></i> Return
                          </button>
                          <%} %> <%if(product.is_returned){%>
                          <span class="bg-warning p-1 rounded"
                            >product returned</span
                          >
                          <%}%>
                        </td>
                        <% const price = product.PriceAfterCategoryDiscount %>

                        <td>
                          <span class="text-secondary"><%=price %></span>
                        </td>

                        <td><%=product.quantity %> PCS</td>
                        <td class="text-end">
                          ₹ <%=price * product.quantity%>
                        </td>
                      </tr>
                      <% }); %>

                      <tr>
                        <td colspan="3"></td>
                        <td colspan="2" class="p-0">
                          <table class="table table-borderless mb-0">
                            <tbody>
                              <tr>
                                <td>Sub Total:</td>
                                <% const subtotal =
                                Math.round((order.totalAmount +
                                (order.totalAmount *order.discount/100 ))) %>
                                <td class="text-end">₹ <%=order.subTotal%></td>
                              </tr>

                              <tr>
                                <td>Discount:</td>

                                <td class="text-end"><%=order.discount %> %</td>
                              </tr>
                              <tr>
                                <td>Shipping Charge (₹10 per KM)</td>
                                <td class="text-end">
                                  + ₹ <%=order.shippingCharge%>
                                </td>
                              </tr>
                              <tr class="border-top">
                                <th>Total (INR) :</th>
                                <th class="text-end">
                                  ₹ <%=order.totalAmount %>
                                </th>
                              </tr>
                            </tbody>
                          </table>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
            <!--end card-->
          </div>
          <!--end col-->

          <div class="col-lg-12">
            <div class="card" id="track-order">
              <div class="card-header d-flex align-items-center gap-3">
                <h5 class="card-title flex-grow-1 mb-0">Order Status</h5>
                <div class="flex-shrink-0">
                  <% if(!order.is_cancelled && order.status !== "Delivered"){ %>
                  <button
                    data-custom-data="<%=order._id %>"
                    data-bs-toggle="modal"
                    data-bs-target="#cancelOrderModal"
                    type="button"
                    class="btn btn-soft-danger btn-sm mt-2 mt-sm-0"
                  >
                    <i
                      class="mdi mdi-archive-remove-outline align-bottom me-1"
                    ></i>
                    Cancel Order
                  </button>
                  <% } %>
                </div>
              </div>
              <div class="card-body">
                <div class="row justify-content-between">
                  <div
                    class="col-lg-2 order-tracking text-center <% if (order.status === 'Pending') { %> completed <% } %>"
                  >
                    <span class="is-complete"></span>
                    <div class="card mt-3 mb-0">
                      <div class="card-body">
                        <h6 class="fs-17">Pending</h6>
                      </div>
                    </div>
                  </div>
                  <!--end col-->
                  <div
                    class="col-lg-2 order-tracking text-center <% if (order.status === 'Processing') { %> completed <% } %>"
                  >
                    <span class="is-complete"></span>
                    <div class="card mt-3 mb-0">
                      <div class="card-body">
                        <h6 class="fs-17">Processing</h6>
                      </div>
                    </div>
                  </div>
                  <!--end col-->
                  <div
                    class="col-lg-2 order-tracking text-center <% if (order.status === 'Shipped') { %> completed <% } %>"
                  >
                    <span class="is-complete"></span>
                    <div class="card mt-3 mb-0">
                      <div class="card-body">
                        <h6 class="fs-17">Order Shipped</h6>
                      </div>
                    </div>
                  </div>
                  <!--end col-->

                  <!--end col-->
                  <div
                    class="col-lg-2 order-tracking text-center <% if (order.status === 'Delivered') { %> completed <% } %>"
                  >
                    <span class="is-complete"></span>
                    <div class="card mt-3 mb-0">
                      <div class="card-body">
                        <h6 class="fs-17">Delivered</h6>
                      </div>
                    </div>
                  </div>
                  <!--end col-->
                </div>
                <!--end row-->
              </div>
            </div>
          </div>
          <!--end container-->
        </div>
      </div>
      <!-- container-fluid -->
    </div>
    <!-- End Page-content -->

    <footer class="footer">
      <div class="container-fluid">
        <div class="row">
          <div class="col-sm-6">
            <script>
              document.write(new Date().getFullYear());
            </script>
            © FASCO.
          </div>
          <div class="col-sm-6"></div>
        </div>
      </div>
    </footer>
  </div>
</div>
<!-- END layout-wrapper -->

<script
  src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/2.5.1/jspdf.umd.min.js"
  integrity="sha512-qZvrmS2ekKPF2mSznTQsxqPgnpkI4DNTlrdUmTzrDgektczlKNRRhy5X5AAOnx5S09ydFYWWNSfcEqDTTHgtNA=="
  crossorigin="anonymous"
  referrerpolicy="no-referrer"
></script>
